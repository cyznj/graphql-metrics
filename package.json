{
  "name": "@workpop/graphql-metrics",
  "version": "0.0.4",
  "description": "GraphQL Instrumentation Tooling",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ava --concurrency=5",
    "test:watch": "npm run test -- --watch",
    "release": "./release.sh",
    "lint": "eslint ./src",
    "flow": "flow check",
    "fix": "eslint . --fix",
    "build": "babel ./src --ignore test --out-dir ./lib",
    "tag-release": "standard-version"
  },
  "ava": {
    "files": [
      "__tests__/*-test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "@workpop/eslint-config-workpop": "^0.0.8",
    "ava": "^0.19.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "flow-bin": "^0.41.0",
    "mocha": "^3.2.0",
    "pre-commit": "^1.2.2",
    "sinon": "^2.1.0",
    "standard-version": "^4.0.0"
  },
  "dependencies": {
    "@workpop/simple-logger": "^1.0.0",
    "babel-runtime": "6.x.x",
    "influx": "^5.0.7",
    "lodash": "^4.17.4"
  },
  "pre-commit": [
    "lint",
    "flow"
  ],
  "author": "Workpop",
  "license": "MIT"
}
