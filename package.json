{
  "name": "@workpop/graphql-metrics",
  "version": "1.0.0",
  "description": "GraphQL Instrumentation Tooling",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "testonly": "mocha test/index.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "release": "./release.sh",
    "lint": "eslint .",
    "flow": "flow check",
    "fix": "eslint . --fix",
    "build": "babel ./src --ignore test --out-dir ./lib",
    "tag-release": "standard-version"
  },
  "devDependencies": {
    "@workpop/eslint-config-workpop": "^0.0.8",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.0",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "flow-bin": "^0.44.2",
    "mocha": "^3.1.2",
    "pre-commit": "^1.1.3",
    "standard-version": "^4.0.0"
  },
  "dependencies": {
    "babel-runtime": "6.x.x",
    "lodash": "^4.17.4"
  },
  "pre-commit": [
    "lint",
    "flow"
  ],
  "author": "Workpop",
  "license": "ISC"
}
